{
  "name": "react-facebook-friends",
  "version": "1.6.0",
  "description": "web app to rank your friendship on Facebook with React",
  "repository": {
    "type": "git",
    "url": "git@github.com:LeoAJ/react-facebook-friends.git"
  },
  "scripts": {
    "jest": "jest",
    "jest:update": "jest --updateSnapshot",
    "flow": "flow",
    "lint": "eslint src/ server.js config/ webpack/ test/",
    "test": "yarn lint && yarn flow && yarn jest",
    "start": "yarn dev",
    "build": "webpack --config webpack/webpack.config.babel.js --progress --colors --define process.env.NODE_ENV='\"production\"'",
    "clean": "rimraf dist/",
    "deploy": "yarn clean && yarn build",
    "dev": "babel-node server.js"
  },
  "author": "Leo Hsieh <leo0924@gmail.com> (http://leoj.js.org)",
  "license": "MIT",
  "keywords": [
    "react",
    "babel",
    "ES6",
    "ES7",
    "async",
    "await",
    "webpack",
    "purecss",
    "Facebook API"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.13.3",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.56.0",
    "flow-typed": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.0",
    "jest": "^21.2.1",
    "json-loader": "^0.5.4",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^16.0.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.5.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "reqwest": "^2.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
